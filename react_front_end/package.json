{
  "name": "stopthevirus-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.9.9",
    "@material-ui/icons": "^4.9.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "firebase-admin": "^8.10.0",
    "history": "^5.0.0-beta.7",
    "material-ui-phone-number": "^2.2.6",
    "netlify-cli": "^2.43.0",
    "netlify-lambda": "^1.6.3",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.13.1",
    "normalize-scss": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.6.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "^6.0.0-alpha.2",
    "react-router-dom": "^6.0.0-alpha.2",
    "react-scripts": "3.4.1",
    "tiktok-scraper": "^1.1.3"
  },
  "scripts": {
    "start": "yarn prepare:lambda-build; run-p start:*",
    "start:app": "react-scripts start",
    "start:lambda": "yarn watch:functions",
    "prepare:lambda-build": "netlify-lambda -c ./webpack.functions.js build ./functions",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "run_lambda": "netlify-lambda -c ./webpack.functions.js build ./functions",
    "watch:functions": "npm-watch run_lambda"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "webpack-node-externals": "^1.7.2"
  },
  "watch": {
    "run_lambda": {
      "patterns": [
        "./functions"
      ],
      "extensions": ".js",
      "quiet": true
    }
  }
}
